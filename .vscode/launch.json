{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Statelessor API",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": "3001"
      },
      "console": "integratedTerminal",
      "restart": true,
      "runtimeExecutable": null,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outputCapture": "std"
    },
    {
      "name": "Launch with Nodemon",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/nodemon/bin/nodemon.js",
      "args": ["server.js"],
      "env": {
        "NODE_ENV": "development",
        "PORT": "3001"
      },
      "console": "integratedTerminal",
      "restart": false,
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "restart": true,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}